{
  "name": "solidjs-auth0",
  "version": "0.0.1",
  "description": "A wrapper on top of auth0-spa-js for solidjs.",
  "author": "Afroze Amjad",
  "keywords": [
    "auth0",
    "solidjs"
  ],
  "license": "MIT",
