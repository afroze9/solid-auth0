{
  "name": "@afroze9/solid-auth0",
  "version": "0.1.1",
  "description": "A wrapper on top of auth0-spa-js for solidjs.",
  "author": "Afroze Amjad",
  "keywords": [
    "auth0",
    "solidjs"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "solid": "./dist/index.jsx",
      "default": "./dist/build/index.js"
    }
  },
  "main": "dist/build/index.cjs.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc&&rollup -c",
    "prebuild": "rimraf dist && npm run format && npm run lint",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.0.4",
    "solid-js": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "babel-preset-solid": "^1.7.1",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "rollup": "^3.20.2",
    "rollup-plugin-filesize": "^10.0.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.0.3"
  }
}
