    "@rollup/plugin-node-resolve": "^15.0.1",
    "babel-preset-solid": "^1.7.1",
    "rimraf": "^4.4.1",
    "rollup": "^3.20.2",
    "rollup-plugin-filesize": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
